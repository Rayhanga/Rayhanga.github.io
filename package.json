{
  "name": "Rayhan-Portofolio-Website",
  "version": "1.1.0",
  "private": true,
  "homepage": "https://rayhanga.github.io",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploydev": "git push origin develop && git push --tags",
    "predeploy": "npm run build",
    "deploy": "push-dir --dir=dist --branch=master --cleanup",
    "v-patch": "npm version patch -m 'Upgrade to %s'",
    "v-major": "npm version major -m 'Upgrade to %s'",
    "v-minor": "npm version minor -m 'Upgrade to %s'"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "firebase": "^5.4.2",
    "material-icons": "^0.2.3",
    "vue": "^2.5.17",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.0.1",
    "vue-truncate-filter": "^1.1.7",
    "vuetify": "^1.2.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@vue/cli": "^3.0.3",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "push-dir": "^0.4.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
